@model Dictionary<string, string>
@{
    ViewBag.Title = "Exceptional";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{ var coordinator = ViewData["coordinator"] as string;}
<h2 style="padding-top:30px">View Exceptional</h2>

<table class="table" style="margin-top: 50px; margin-bottom:50px">
    <thead class="thead-dark">
        <tr>
            <th>
                Student
            </th>
            <th>
                Mail
            </th>
            <th>
                Submissions
            </th>
        </tr>
    </thead>
    
    @foreach (var item in Model)
    {
<tr>
    <td>
        @item.Key
    </td>
    <td>
        @item.Value
    </td>
    <td>
        @Html.ActionLink("View", "viewSubmissions", new { coordinator = @coordinator, Student = @item.Key }, new { @class = "btn btn-primary"})
    </td>
</tr>
    }

    
    


</table>