@model Dictionary<string, string>
@{
    ViewBag.Title = "Exceptional";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{ var coordinator = ViewData["coordinator"] as string;}
<h2>Exceptional</h2>
@Html.ActionLink("Back to index", "Index")
<table class="table">
    <tr>
        <th>
            Student
        </th>
        <th>
            Mail
        </th>
        <th>
            Submissions
        </th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>
        @item.Key
    </td>
    <td>
        @item.Value
    </td>
    <td>
        @Html.ActionLink("View submissions", "viewSubmissions", new { coordinator = @coordinator, Student = @item.Key })
    </td>
</tr>
    }

    
    


</table>