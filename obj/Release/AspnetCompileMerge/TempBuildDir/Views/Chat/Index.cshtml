@model Dictionary<string,string>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<table>
    <tr>
        <td>
            Email
        </td>
        <td>
            Chat
        </td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>
                @item.Value
            </td>
            <td>
                @Html.ActionLink("Chat", "Chat", new { id = item.Key })
            </td>
            
        </tr>
    }
    </table>

