@model IEnumerable<WebApplication1.Models.Course>

@{
    ViewBag.Title = "Index Course";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index Course</h2>

<table class="table">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nameCourse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coordinator)
        </th>


        <th>
            @Html.DisplayNameFor(model => model.dateEnd)
        </th>
        <th>
            View Mark
        </th>

        <th>
            Edit
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.nameCourse)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Coordinator)
    </td>


    <td>
        @Html.DisplayFor(modelItem => item.dateEnd)
    </td>
    <td>
        @Html.ActionLink("View", "ViewMark", new { coordinator = item.Coordinator })

    </td>
    <td>
        @Html.ActionLink("Edit Date End ", "EditCourse", new { coordinator = item.Coordinator })

    </td>
</tr>
}

</table>
