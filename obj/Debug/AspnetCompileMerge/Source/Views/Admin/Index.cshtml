@model IEnumerable<WebApplication1.Models.SignUpModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create Admin", "CreateAdmin") | @Html.ActionLink("Create Student", "CreateStudent") | @Html.ActionLink("Create Marketing Coordinator", "CreateMarketingCoordinator") | @Html.ActionLink("Create Marketing Manager", "CreateMarketingManager") | @Html.ActionLink("Create Guest", "CreateGuest")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.role)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.enable)
        </th>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Password)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.role)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.enable)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.id, role = item.role })
        


    </td>
    <td>
        @Html.ActionLink("Delete", "Delete", new { id = item.id, role = item.role })
    </td>
</tr>
}

</table>
